{
  "manifest_version": 2,

  "name": "Anti Phishing",
  "description": "Protects against phishing",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["antiph.js"],
      "run_at":"document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}